{
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "server/api/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "server/api/pages/login.ts",  
    "server/api/index.ts",
    "server/authMiddleware.ts",
    "server/AuthenticatedJWT.ts",
    "server/protectedRoutes.ts",
    "server/config.ts",  
    "server/userInterface.ts"  
, "app/Client/SendEmail", "app/SellerDashboard/Components/Input.tsx"  ],
  "exclude": [
    "node_modules"
    // "server/api/index.ts"  // Consider removing this line unless you want to exclude it
  ]
}